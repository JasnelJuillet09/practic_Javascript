function diffArray(arr1, arr2) {
  var newArr = [];
  function onlyInFirst( a1, a2){
    for(let i = 0; i < a1.length; i++ ){

    if(a2.indexOf(a1[i]) === -1){
     newArr.push(a1[i]);
    }
  }
  }
  onlyInFirst(arr1, arr2);
  onlyInFirst(arr2, arr1);
  return newArr;
}

diffArray([1, 2, 3, 5], [1, 2, 3, 4, 5]);
let a = [1, 2, 3, 5];
let b = [1, 2, 3, 4, 5];
console.log(b.indexOf(a));
